{
  "functions": {
    "src/entry-server.tsx": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/_vercel/insights/(.*)",
      "dest": "/_vercel/insights/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}